{
  "name": "krenor/prometheus-client",
  "description": "A PHP Client for Prometheus",
  "keywords": [
    "metrics",
    "monitoring",
    "prometheus"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Stanislav Goldmann",
      "email": "stanislav.goldmann@gmail.com",
      "role": "Developer",
      "homepage": "https://github.com/krenor"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^7.1",
    "tightenco/collect": "^5.6"
  },
  "require-dev": {
    "codedungeon/phpunit-result-printer": "^0.16",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.0",
    "predis/predis": "^1.1"
  },
  "suggest": {
    "ext-apcu": "Required to use the APCu storage.",
    "predis/predis": "Required to use the Redis storage."
  },
  "autoload": {
    "psr-4": {
      "Krenor\\Prometheus\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Krenor\\Prometheus\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Krenor\\Preometheus\\Providers\\LaravelServiceProvider"
      ]
    }
  }
}
